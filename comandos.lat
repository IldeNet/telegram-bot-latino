elegir(msg->texto)

 caso "/start":
	enviarMensaje(chat->id, "¡Hey! ".usuario->nombre."\n\nEstás probando el nuevo bot escrito completamente en la *programación Latino*. Por el momento faltan cosas. Pero de poco a poco irá mejorando 😄👍", verdadero, falso)

  
 caso "/id":
	enviarMensaje(chat->id, "*Id* ".usuario->id."\n*Chat Id* ".chat->id, verdadero, msg->id)

 caso "/foto":
 	enviarFoto(chat->id, "https://raw.githubusercontent.com/lenguaje-latino/latino-sublimetext-plugin/master/latino_sublime.png", "Sublime Text sintaxis Latino", falso, falso)

 caso "#miembros":
 	miembros = obtenerCuentaMiembros(chat->id)
 	enviarMensaje(chat->id, "Miembros\n*".miembros."*", verdadero, msg->id)
fin

elegir(inline->consulta)
  caso "latino":
  	 latino = retornarConsultaInline("articulo", inline->id, "GitHub Latino", "[latino](https://github.com/primitivorm/latino)", "Aprende a programar con Latino")
  	 enviarConsultaInline(inline->id, latino)
fin

si nuevo_usuario->id
	enviarMensaje(chat->id, "Hola ".nuevo_usuario->nombre.", bienvenido a ".chat->titulo, falso, falso)
fin

si msg->texto
	escribir(msg->texto." desde ".usuario->nombre)
fin
